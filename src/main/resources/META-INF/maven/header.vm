#parse( "header-macros.vm" )
<header id="site-header" class="header">
  <div id="header-wrapper" class="wrapper">
    <a class="site-logo custom-logo-link" href="index.html">
      <img class="site-icon custom-logo"  src="$relativePath/images/site-icon.png" alt="Site Icon" />
      <h1 id="site-title" class="custom-logo-title">CoderDojo</h1>
    </a>
    <nav id="desktop-navigation" class="menu-desktop-navigation-menu-container">
      #*   *##headerMenu( $decoration.body.menus )
    </nav>
    <a id="header-cta" href="{{ site.header_cta_url }}">$decoration.name</a>
    <button id="mobile-menu-toggle" class="toggle-button" onclick="toggleMobileMenu()">&#9776;</button>
  </div>
</header>